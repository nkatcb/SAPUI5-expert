<mvc:View controllerName="dtt.project1.controller.HelloPanel" xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns:core="sap.ui.core">
    <Panel id="_IDGenPanel1" headerText="{i18n>helloPanel}" class="sapUiSmallMargin" width="auto">
        <content>
            <Button id="_IDGenButton2" text="{i18n>HelloDialogButton}" press=".onOpenDialog" class="sapUiSmallMarginEnd" icon="sap-icon://action" />
            <Button id="_IDGenButton1" text="{i18n>showHelloButtonText}" press=".onShowHello" class="sapUiSmallMarginEnd" />
            <Input id="_IDGenInput1" value="{/recipient/name}" valueLiveUpdate="true" width="60%" />
            <Text id="_IDGenText1" text="hello{/recipient/name}" class="sapUiSmallMargin boldElements sapthemeHighlight-asColor" />
        </content>
    </Panel>
    <core:Fragment id="_IDGenFragment1" fragmentName="dtt.project1.view.Form01" type="XML" />
</mvc:View>
